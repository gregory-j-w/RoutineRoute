<div class = "show-container">
  <div class = "show-left-div">
  </div>
  <div class = "show-main-div">
    <div class = "main-flex-container">
      <div class = "section-title">
        <h2> <%= "#  " + @rt + " " + @rtdir + " Stops" %> </h2>
      </div>
      <div class = "login-container">
        <div class = "button">
          <ul class = "route-ul">
            <% @busstops.each do |s| %>
              <li>
                <%= link_to s["stpnm"], bussave_path(rt: @rt, rtnm: @rtnm, rtdir: @rtdir, stpid: s["stpid"], stpnm: s["stpnm"]), class: "route-button" %>
              </li>
            <% end %>
          </ul>
        </div>
      </div>
    </div>
  </div>
  <div class = "show-right-div">
  </div>
</div>
