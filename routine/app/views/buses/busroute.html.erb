<div class = "show-container">
  <div class = "show-left-div">
  </div>
  <div class = "show-main-div">
    <div class = "main-flex-container">
      <div class = "section-title">
        <h2>CTA Bus Routes<h2>
      </div>
      <div class = "login-container">
        <div class = "button">
          <ul class = "route-ul">
          <% @busroutes.each do |r| %>
            <li>
              <%= link_to "#"  + r["rt"] + " " + r["rtnm"], busdirection_path(rt: r["rt"], rtnm: r["rtnm"]),  class: "route-button" %>
            </li>
          <% end %>
          </ul>
        </div>
      </div>
    </div>
  </div>
  <div class = "show-right-div">
  </div>
</div>
</div>
